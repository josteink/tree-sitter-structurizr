        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "\""
          },
          {
            "type": "PATTERN",
            "value": "[^\"$\\\\]+"
          },
          {
            "type": "STRING",
            "value": "\""
          }
        ]
      }
    },
